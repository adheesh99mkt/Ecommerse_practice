@using Ecommerse_practice.Models;
@{
    List<Products> products = (List<Products>)ViewData["catalog"];
    <h1>List of Products</h1>
    <ol>
        @{
            foreach(Products product in products)
            {
                <li>
                @product.Title
                @product.Description
                @product.UnitPrice
                @product.ImageUrl
                @product.Quantity
                </li>
            }
        }
    </ol>
}
